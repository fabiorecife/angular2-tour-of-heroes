// Place your settings in this file to overwrite default and user settings.
{

    // Configure glob patterns for excluding files and folders.
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "app/**/.spec.ts": true,
        "app/**/.spec.js.map": true,
        "app/**/.js": { "when": "$(basename).ts"},
        "app/**/.js.map": true,
        "app/**/.spec.js": true
    }
}